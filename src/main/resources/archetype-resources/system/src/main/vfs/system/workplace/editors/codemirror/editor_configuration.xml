#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="ISO-8859-1"?>
<editor>
	<label>CodeMirror</label>
	<resourcetypes>
		<type>
			<name>plain</name>
			<ranking>20</ranking>
		</type>
		<type>
			<name>jsp</name>
			<ranking>20</ranking>
			<mapto>plain</mapto>
		</type>
        <type>
			<name>containerpage_template</name>
			<ranking>20</ranking>
			<mapto>plain</mapto>
		</type>
	</resourcetypes>
	<useragents>
		<agent>^Mozilla/[45]${symbol_escape}.0 ${symbol_escape}(compatible; MSIE .*${symbol_escape})${symbol_dollar}</agent> 
		<agent>^Mozilla/5${symbol_escape}.0 ${symbol_escape}(.*${symbol_escape}) Gecko/20${symbol_escape}d* .*${symbol_dollar}</agent>
		<agent>^Mozilla/5${symbol_escape}.0 ${symbol_escape}(.*${symbol_escape}).*Chrome.*${symbol_dollar}</agent>
		<agent>^Mozilla/5${symbol_escape}.0 ${symbol_escape}(.*${symbol_escape}).*Safari.*${symbol_dollar}</agent>
		<agent>^Opera/9${symbol_escape}..*${symbol_escape}(.*${symbol_escape}).*Presto.*${symbol_dollar}</agent>
	</useragents>
</editor>